{
  "name": "python-scripting-editor",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run dev:knime",
    "dev:knime": "vite --mode development.knime",
    "dev:browser": "vite --mode development.browser",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "lint": "run-p 'lint:* -- --fix'",
    "lint:js": "eslint . --cache --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:css": "stylelint --cache '**/*.{css,vue}'",
    "ci:lint-format": "run-p ci:lint:* format:check -c",
    "ci:lint:js": "npm run lint:js && npm run lint:js -- -f json -o test-results/eslint.json",
    "ci:lint:css": "npm run lint:css -- -f json -o test-results/stylelint.json",
    "jenkins:lint": "eslint --cache --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts -f json -o target/eslint.json || true; stylelint '**/*.css' -f json -o target/stylelint.json || true",
    "audit": "npm audit --omit dev",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "cd ../.. && husky install ./org.knime.python3.scripting.nodes/js-src/.husky"
  },
  "dependencies": {
    "@knime/scripting-editor": "0.0.26",
    "@knime/ui-extension-service": "0.18.0",
    "consola": "2.15.3",
    "pinia": "2.1.4",
    "vue": "3.3.4",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@knime/eslint-config": "8.0.7",
    "@rushstack/eslint-patch": "1.3.2",
    "@tsconfig/node18": "18.2.0",
    "@types/jsdom": "21.1.1",
    "@types/node": "18.17.0",
    "@vitejs/plugin-vue": "4.2.3",
    "@vitejs/plugin-vue-jsx": "3.0.1",
    "@vitest/coverage-v8": "0.34.1",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/eslint-config-typescript": "11.0.3",
    "@vue/test-utils": "2.4.1",
    "@vue/tsconfig": "0.4.0",
    "eslint": "8.45.0",
    "eslint-plugin-vue": "9.15.1",
    "husky": "^8.0.3",
    "jsdom": "22.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.0",
    "stylelint": "15.10.2",
    "typescript": "5.1.6",
    "vite": "4.4.6",
    "vite-plugin-monaco-editor": "1.1.0",
    "vite-svg-loader": "4.0.0",
    "vitest": "0.34.1",
    "vitest-canvas-mock": "0.3.3",
    "vue-tsc": "1.8.8"
  },
  "engines": {
    "node": "18.x"
  },
  "volta": {
    "node": "18.17.0"
  }
}
