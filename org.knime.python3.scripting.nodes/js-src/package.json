{
  "name": "python-scripting-editor",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run dev:knime",
    "dev:knime": "vite",
    "dev:browser": "vite --mode development.browser",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "lint": "run-p 'lint:* -- --fix'",
    "lint:js": "eslint . --cache --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:css": "stylelint --cache '**/*.{css,vue}'",
    "ci:lint-format": "run-p ci:lint:* format:check -c",
    "ci:lint:js": "npm run lint:js && npm run lint:js -- -f json -o test-results/eslint.json",
    "ci:lint:css": "npm run lint:css -- -f json -o test-results/stylelint.json",
    "jenkins:lint": "eslint . --cache --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts -f json -o target/eslint.json || true; stylelint '**/*.css' -f json -o target/stylelint.json || true",
    "audit": "npm audit --omit dev",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "cd ../.. && husky install ./org.knime.python3.scripting.nodes/js-src/.husky",
    "sbom": "npx @cyclonedx/cyclonedx-npm --output-file bom.json --ignore-npm-errors --omit dev"
  },
  "dependencies": {
    "@knime/scripting-editor": "0.0.57",
    "@knime/ui-extension-service": "0.30.0",
    "@vueuse/core": "10.9.0",
    "consola": "2.15.3",
    "vue": "3.4.21",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@cyclonedx/cyclonedx-npm": "^1.16.2",
    "@knime/eslint-config": "8.0.14",
    "@rushstack/eslint-patch": "1.9.0",
    "@tsconfig/node18": "18.2.4",
    "@types/jsdom": "21.1.6",
    "@types/node": "20.11.30",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitest/coverage-v8": "1.4.0",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/test-utils": "2.4.5",
    "@vue/tsconfig": "0.5.1",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.24.0",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "stylelint": "16.3.1",
    "typescript": "5.4.3",
    "vite": "5.2.6",
    "vite-plugin-monaco-editor": "1.1.0",
    "vite-svg-loader": "5.1.0",
    "vitest": "1.4.0",
    "vitest-canvas-mock": "0.3.3",
    "vue-tsc": "2.0.7"
  },
  "engines": {
    "node": "20.x"
  },
  "volta": {
    "node": "20.11.1"
  }
}
