{
  "name": "python-scripting-editor",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run dev:knime",
    "dev:knime": "APP_ENV=knime vite",
    "dev:browser": "APP_ENV=browser vite",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "lint": "run-p 'lint:* -- --fix'",
    "lint:js": "eslint . --cache --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:css": "stylelint --cache '**/*.{css,vue}'",
    "ci:lint-format": "run-p ci:lint:* format:check -c",
    "ci:lint:js": "npm run lint:js && npm run lint:js -- -f json -o test-results/eslint.json",
    "ci:lint:css": "npm run lint:css -- -f json -o test-results/stylelint.json",
    "jenkins:lint": "eslint --cache --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts -f json -o target/eslint.json || true; stylelint '**/*.css' -f json -o target/stylelint.json || true",
    "audit": "npm audit --omit dev",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "cd ../.. && husky install ./org.knime.python3.scripting.nodes/js-src/.husky"
  },
  "dependencies": {
    "@knime/scripting-editor": "0.0.48",
    "@knime/ui-extension-service": "0.22.0",
    "@vueuse/core": "10.7.1",
    "consola": "2.15.3",
    "vue": "3.3.4",
    "webapps-common": "file:webapps-common"
  },
  "devDependencies": {
    "@knime/eslint-config": "8.0.10",
    "@rushstack/eslint-patch": "1.6.1",
    "@tsconfig/node18": "18.2.2",
    "@types/jsdom": "21.1.6",
    "@types/node": "18.19.5",
    "@vitejs/plugin-vue": "5.0.2",
    "@vitest/coverage-v8": "1.1.3",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/test-utils": "2.4.3",
    "@vue/tsconfig": "0.5.1",
    "eslint": "8.56.0",
    "eslint-plugin-vue": "9.19.2",
    "husky": "8.0.3",
    "jsdom": "23.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.1",
    "stylelint": "16.1.0",
    "typescript": "5.3.3",
    "vite": "5.0.11",
    "vite-plugin-monaco-editor": "1.1.0",
    "vite-svg-loader": "5.1.0",
    "vitest": "1.1.3",
    "vitest-canvas-mock": "0.3.3",
    "vue-tsc": "1.8.27"
  },
  "engines": {
    "node": "18.x"
  },
  "volta": {
    "node": "18.19.0"
  }
}
